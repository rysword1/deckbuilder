{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://our.deck.com/deckNew.schema.json",
    "type": "object",
    "properties": {
        "title": {
            "type": "string",
            "minLength": 1,
            "maxLength": 30
        },
        "dateCreated": {
            "type": "string",
            "format": "date"
        },
        "card_ids": {
            "type": "string"
        }
    },
    "additionalProperties": false,
    "required": [
        "title",
        "dateCreated"
    ]
}